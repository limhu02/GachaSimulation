<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.springboot.sample4.mapper.MyMapper">
	<select id="findAll" resultType="com.springboot.sample4.model.Guail">
	select seqno, title, price from guail_tbl
	</select>
	
	<select id="getGuail" parameterType="Integer" resultType="com.springboot.sample4.model.Guail">
		select seqno, title, price, info, image_name from guail_tbl where seqno=#{itemId}
	</select>
</mapper>