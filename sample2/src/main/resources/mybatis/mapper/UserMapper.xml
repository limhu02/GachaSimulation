<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.springboot.sample2.mapper.UserMapper">

	<select id="getUsers" resultType="com.springboot.sample2.model.User_info">
	select user_id,user_pwd,name,addr,phone,gender,grade,email,birth,job from user_info
	</select>
	<select id="getUserDetail" parameterType="String"  resultType="com.springboot.sample2.model.User_info">
	 select user_id,user_pwd,name,addr,phone,gender,grade,email,birth,job from user_info where user_id =#{id}
	</select>

</mapper>